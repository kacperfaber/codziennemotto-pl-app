{
  "name": "pl.codziennemotto",
  "displayName": "codziennemotto.pl",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "webpack-dev": "npx webpack --config webpack.config.dev.js",
    "fontello-open": "fontello-cli --config www/icons/config.json open",
    "fontello-install": "fontello-cli -- config www/icons/config.json --css www/icons/css --font www/icons/font install",
    "run-dev": "npm run webpack-dev && cordova run"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Kacper Faber",
  "license": "Apache-2.0",
  "dependencies": {
    "bootstrap": "^5.2.3",
    "i18next": "^22.4.15",
    "mithril": "^2.2.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/mithril": "^2.0.12",
    "cordova-browser": "^6.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  },
  "cordova": {
    "platforms": [
      "browser"
    ]
  }
